{
    "compilerOptions": {
        "noImplicitAny": false,
        "esModuleInterop": true,
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "module": "ES2020",
        "moduleResolution": "Node",
        "isolatedModules": true,
        "resolveJsonModule": true,
        "jsx": "preserve",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "incremental": true,
        "noFallthroughCasesInSwitch": true
    }
}